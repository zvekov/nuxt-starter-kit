{
  "name": "nuxt-starter-kit",
  "scope": "zvekov",
  "trailingSlash": true,
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/now-builder",
      "config": {
        "serverFiles": [
          "./.nuxt/dist/sitemap-routes.json",
          "middleware/**",
          "plugins/**",
          "lib/**",
          "package.json",
          "static/**"
        ]
      }
    }
  ]
}
